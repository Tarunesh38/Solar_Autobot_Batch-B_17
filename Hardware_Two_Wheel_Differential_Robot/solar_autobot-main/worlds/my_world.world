<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">

    <!-- Define the sun -->
    <model name="sun">
      <pose>0 0 1000 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <sphere>
              <radius>10</radius>
            </sphere>
          </geometry>
          <material>
            <script>
              <uri>model://sun/materials/scripts</uri>
              <uri>model://sun/materials/textures</uri>
              <name>sun/yellow</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Define buildings -->
    <model name="building1">
      <pose>10 10 0 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>10 10 100</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>model://building1/materials/scripts</uri>
              <uri>model://building1/materials/textures</uri>
              <name>building1/gray</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <model name="building2">
      <pose>-10 20 0 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>8 12 120</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>model://building2/materials/scripts</uri>
              <uri>model://building2/materials/textures</uri>
              <name>building2/gray</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Add more buildings -->
    <model name="building3">
      <pose>20 -15 0 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>15 8 90</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>model://building3/materials/scripts</uri>
              <uri>model://building3/materials/textures</uri>
              <name>building3/gray</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Define grass -->
    <model name="grass">
      <pose>0 0 0 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>100 100 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>model://grass/materials/scripts</uri>
              <uri>model://grass/materials/textures</uri>
              <name>grass/green</name>
