    <model name="footpath2">
      <pose>5 -20 0 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>2 40 0.1</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>model://footpath2/materials/scripts</uri>
