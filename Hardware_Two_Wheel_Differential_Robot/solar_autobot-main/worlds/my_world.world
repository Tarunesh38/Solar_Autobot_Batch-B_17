    </model>

    <!-- Define trees -->
    <model name="tree1">
      <pose>-15 15 0 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>1</radius>
              <length>10</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <uri>model://tree1/materials/scripts</uri>
              <uri>model://tree1/materials/textures</uri>
